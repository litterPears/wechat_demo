<!--components/main/main.wxml-->
<scroll-view class="scrollView"
  scroll-x="{{options.scrollX}}" 
  upper-threshold="{{options.upperThreshold}}" 
  scroll-with-animation="{{options.scrollWithAnimation}}" 
  enable-back-to-top="enableBackToTop">
  <view class="scrollMain">
    <components-table wx:for="{{tablesList}}" default-message="{{item}}" wx:key="index"></components-table>
  </view>
</scroll-view>
