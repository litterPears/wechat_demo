/* components/rightNav/rightnav.wxss */
.rightmains{
  height:auto;
  padding:0 30rpx;
  overflow:hidden;
  margin-bottom:50rpx;
}
.mains{
  width:100%;
  display:flex;
  justify-content:flex-start;
  flex-wrap: wrap;
}
.rightTitle{
  height:50rpx;
  line-height:50rpx;
  text-align:center;
  color:#404040;
  font-size:30rpx;
  margin-top:20rpx;
  border-bottom:2rpx solid #eee;
}
.picks{
  width:50%;
  text-align: center;
  margin:20rpx 0 20rpx 0;
}
.imgSrc{
  width:200rpx;
  height:200rpx;
  display:block;
  margin:0 auto;
  border:2rpx solid #eee;
}
.titles{
  line-height:40rpx;
  text-align:center;
  font-size:24rpx;
  color:#404040;
}
.notexts{
  line-height:200rpx;
  text-align:center;
  font-size:25rpx;
  color:#aaa;
}