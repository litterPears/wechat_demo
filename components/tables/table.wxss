/* components/tables/table.wxss */
.tableMain{
  width:100%;
  height:auto;
}
.banners{
  width:100%;
  height:150rpx;
}
.banners .images{
  display:block;
  width:100%;
  height:150rpx;
  margin:20rpx auto;
}
.bannerTitle{
  height:100rpx;
  padding:0 30rpx;
  display:flex;
  justify-content:space-between;
  align-items: center;
  font-size:24rpx;
  color:#404040;
}
.bannerTitle .titles{
  display:flex;
  justify-content:flex-start;
  align-items: center;
}
.bannerTitle .titleIcon{
  width:40rpx;
  height:40rpx;
  margin-right:20rpx;
}
.listMain{
  display:flex;
  justify-content:space-between;
  align-items: center;
  min-height:280rpx;
  overflow:hidden;
  margin:20rpx auto 30rpx;
  padding:0 28rpx;
}
.listImg{
  width:200rpx;
  height:200rpx;
  display:block;
  margin-right:30rpx;
}
.listTitles{
  width:400rpx;
  height:280rpx;
}
.gatName{
  font-size:30rpx;
  line-height:40rpx;
}
.gatTitles{
  width:400rpx;
  font-size:24rpx;
  color:#404040;
  line-height:40rpx;
  text-align:justify;
  overflow:hidden;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:3;
}
.payNumber{
  height:40rpx;
  line-height:40rpx;
  font-size:20rpx;
  margin:20rpx auto 30rpx;
  display:flex;
  justify-content:space-between;
  padding-right:40rpx;
}
.contents{
  overflow:hidden;
  line-height:40rpx;
}
.cungat{
  font-size:24rpx;
  color:#ffbe00;
  line-height:50rpx;
}